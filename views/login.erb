<html>
  <head>
    <title>Abiquo 2 factor authentication</title>
    <style>
      @import url(http://fonts.googleapis.com/css?family=Exo:100,200,400);
      @import url(http://fonts.googleapis.com/css?family=Source+Sans+Pro:700,400,300);
    </style>
    <link rel="stylesheet" href="/abiquo2fa.css">
  </head>
  <body>


  <div class="body"></div>
    <div class="grad"></div>
    <div class="header">
      <div>Abiquo<br><span><strong>twoFactorAuthentication</strong></span></div>
    </div>
    <br>
    <div class="login">
      <form action="/token" method="post">
        <input id="username" type="text" name="username" placeholder="Abiquo username" focus><br>
        <input id="email" type="email" name="email" placeholder="Email address"><br>
        <input type="submit" value="Request token">
      </form>
    </div>


  </body>
</html>